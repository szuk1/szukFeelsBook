<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/EmotionCountTotal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EmotionCount">

    <TextView
        android:id="@+id/EmotionCountTextview"
        android:layout_width="216dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:text="Emotion Count"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Emotion Count" />

    <TextView
        android:id="@+id/TotalEmotionTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="104dp"
        android:text="Total"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/TotalLoveTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:text="Love"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TotalEmotionTextview" />

    <TextView
        android:id="@+id/TotalJoyTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:text="Joy"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TotalLoveTextview" />

    <TextView
        android:id="@+id/TotalSurpriseTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:text="Surprise"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TotalJoyTextview" />

    <TextView
        android:id="@+id/TotalAngryTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:text="Anger"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TotalSurpriseTextview" />


    <TextView
        android:id="@+id/TotalFearTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:text="Fear"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TotalSadnessTextview" />

    <TextView
        android:id="@+id/TotalEmotionCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="104dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.379"
        app:layout_constraintStart_toEndOf="@+id/TotalEmotionTextview"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/LoveCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.377"
        app:layout_constraintStart_toEndOf="@+id/TotalLoveTextview"
        app:layout_constraintTop_toBottomOf="@+id/TotalEmotionCount" />

    <TextView
        android:id="@+id/JoyCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toEndOf="@+id/TotalJoyTextview"
        app:layout_constraintTop_toBottomOf="@+id/LoveCount" />

    <TextView
        android:id="@+id/SurpriseCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toEndOf="@+id/TotalSurpriseTextview"
        app:layout_constraintTop_toBottomOf="@+id/JoyCount" />

    <TextView
        android:id="@+id/SadnessCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toEndOf="@+id/TotalSadnessTextview"
        app:layout_constraintTop_toBottomOf="@+id/AngerCount" />

    <TextView
        android:id="@+id/AngerCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toEndOf="@+id/TotalAngryTextview"
        app:layout_constraintTop_toBottomOf="@+id/SurpriseCount" />

    <TextView
        android:id="@+id/FearCount"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toEndOf="@+id/TotalFearTextview"
        app:layout_constraintTop_toBottomOf="@+id/SadnessCount" />

    <TextView
        android:id="@+id/TotalSadnessTextview"
        android:layout_width="81dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:text="Sadness"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TotalAngryTextview" />
</android.support.constraint.ConstraintLayout>